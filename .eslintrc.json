{
	"root": true,
	"extends": [
		"eslint:recommended"
	],
	"env": {
		"es2022": true,
		"browser": true,
		"worker": true,
		"node": true
	},
	"overrides": [
		{
			"files": ["worker/**/*.js"],
			"env": { "worker": true, "browser": false }
		},
		{
			"files": ["admin/**/*.jsx", "admin/**/*.js"],
			"env": { "browser": true }
		}
	],
	"parserOptions": {
		"sourceType": "module"
	}
}


